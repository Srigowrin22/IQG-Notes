package training.iqgateway.servicesimpl;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import training.iqgateway.entities.AdminEO;
import training.iqgateway.repository.AdminEORepository;
import training.iqgateway.services.AdminEOService;

@Service
public class AdminEOServiceImpl implements AdminEOService {

	
	
	@Autowired
	private AdminEORepository  adminEORepository;
	@Override
	public List<AdminEO> getAll() {
		return adminEORepository.findAll();
	}

	@Override
	public AdminEO getByIdd(String designationId) {	
		return  adminEORepository.findBydesignationId(designationId) ;
	}
	
	


	@Override
	public AdminEO updateAdmin(AdminEO adminEO) {
		return adminEORepository.save(adminEO);
	}

	@Override
	public AdminEO addAdmin(AdminEO adminEO) {
		return adminEORepository.save(adminEO);
		
	}

	@Override
	public void deleteAdmin(String id) {
		return adminEORepository.deleteById(id);

	}

	@Override
	public void delete(AdminEO admin) {
		// TODO Auto-generated method stub

	}

}
