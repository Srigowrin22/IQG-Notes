<?xml version='1.0' encoding='windows-1252'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:h="http://java.sun.com/jsf/html">
  <jsp:output omit-xml-declaration="true" doctype-root-element="HTML"
              doctype-system="http://www.w3.org/TR/html4/loose.dtd"
              doctype-public="-//W3C//DTD HTML 4.01 Transitional//EN"/>
  <jsp:directive.page contentType="text/html;charset=windows-1252"/>
  <f:view>
    <html>
      <head>
        <meta http-equiv="Content-Type"
              content="text/html; charset=windows-1252"/>
        <title>Owner Details</title>
        <link rel="stylesheet" type="text/css" href="images/styles1.css"/>
      </head>
      <body>
       <!-- Header panel -->
        <div class="header">
          <div class="header-left"></div>
          <div class="header-title">VEHICLE OFFENCE MANAGEMENT SYSTEM</div>
          <div class="header-right">
            <h:form style="margin:0; padding:0;">
              <h:commandButton value="Logout" styleClass="logout-btn"
                               action="#{backing_ClerkDashboard.logout_action}"/>
            </h:form>
          </div>
        </div>
        <!-- Side panel -->
        <div class="main-layout">
          <div class="sidebar">
            <div class="sidebar-title">Owner Details</div>
            <div class="form-row">
              <h:outputLabel value="Designation ID" for="desigID"
                             styleClass="sidebar-label"/>
               
              <h:inputText value="#{backing_ClerkDashboard.designationId}"
                           id="desigID" readonly="true"
                           styleClass="sidebar-input"/>
            </div>
             <h:form id="sidebarNavForm">
              <div class="sidebar-nav">
                <h:commandLink value="Home"
                               action="#{backing_ClerkDashboard.home_action}"
                               styleClass="sidebar-nav-link"/>
                 
                <h:commandLink value="Raise Offence"
                               action="#{backing_ClerkDashboard.raiseOffence_action}"
                               styleClass="sidebar-nav-link"/>
                 
                <h:commandLink value="Pending Offences"
                               action="#{backing_ClerkDashboard.pendingOffences_action}"
                               styleClass="sidebar-nav-link"/>
                 
                <h:commandLink value="View Owner"
                               action="#{backing_ClerkDashboard.viewOwner_action}"
                               styleClass="sidebar-nav-link"/>
                 
                <h:commandLink value="Generate reports"
                               action="#{backing_ClerkDashboard.generateReports_action}"
                               styleClass="sidebar-nav-link"/>
                 
                <h:commandLink value="Offence Types"
                               action="#{backing_ClerkDashboard.offenceTypes_action}"
                               styleClass="sidebar-nav-link"/>
              </div>
            </h:form>
          </div>
          <div class="center-content">
            <!-- Message display -->
            <h:panelGroup rendered="#{not empty backing_OwnerDetails.message}">
              <div class="admin-msg">
                <h:outputText value="#{backing_OwnerDetails.message}"/>
              </div>
            </h:panelGroup>
            <div class="raise-center-wrapper-owner">
              <h:form id="ownerDetailsForm">
                <!-- 1. Search Card: always visible -->
                <div class="card search-card">
                  <h:outputText value="Search Registration"
                                styleClass="card-header"/>
                  <div class="field-row">
                    <h:outputLabel for="searchRegID" value="Registration ID:"
                                   styleClass="field-label"/>
                     
                    <h:inputText id="searchRegID"
                                 value="#{backing_OwnerDetails.searchRegistrationID}"
                                 styleClass="field-input"/>
                     
                    <h:commandButton value="Search"
                                     action="#{backing_OwnerDetails.search_action}"
                                     styleClass="search-button"/>
                  </div>
                </div>
                <!-- 2. Owner Details Card -->
                <h:panelGroup layout="block" styleClass="card owner-card"
                              rendered="#{backing_OwnerDetails.showCard}">
                  <h:outputText value="Owner Details" styleClass="card-header"/>
                  <div class="two-column-row">
                    <div class="field-row">
                      <h:outputLabel for="ownerName" value="Owner Name:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="ownerName"
                                    value="#{backing_OwnerDetails.ownerName}"
                                    styleClass="field-output"/>
                    </div>
                    <div class="field-row">
                      <h:outputLabel for="phone" value="Phone:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="phone"
                                    value="#{backing_OwnerDetails.phone}"
                                    styleClass="field-output"/>
                    </div>
                  </div>
                  <div class="two-column-row">
                    <div class="field-row">
                      <h:outputLabel for="aadhar" value="Aadhar:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="aadhar"
                                    value="#{backing_OwnerDetails.aadhar}"
                                    styleClass="field-output"/>
                    </div>
                    <div class="field-row">
                      <h:outputLabel for="pancard" value="Pancard:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="pancard"
                                    value="#{backing_OwnerDetails.pancard}"
                                    styleClass="field-output"/>
                    </div>
                  </div>
                  <div class="two-column-row">
                    <div class="field-row">
                      <h:outputLabel for="address" value="Address:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="address"
                                    value="#{backing_OwnerDetails.address}"
                                    styleClass="field-output"/>
                    </div>
                    <div class="field-row">
                      <h:outputLabel for="gender" value="Gender:"
                                     styleClass="field-label"/>
                       
                      <h:selectOneRadio id="gender"
                                        value="#{backing_OwnerDetails.gender}"
                                        styleClass="field-radio"
                                        disabled="true">
                        <f:selectItem itemLabel="Male" itemValue="M"/>
                        <f:selectItem itemLabel="Female" itemValue="F"/>
                        <f:selectItem itemLabel="Other" itemValue="NA"/>
                      </h:selectOneRadio>
                    </div>
                  </div>
                </h:panelGroup>
                <!-- 3. Registration Details Card -->
                <h:panelGroup layout="block" styleClass="card reg-card"
                              rendered="#{backing_OwnerDetails.showCard}">
                  <h:outputText value="Registration Details"
                                styleClass="card-header"/>
                  <div class="two-column-row">
                    <div class="field-row">
                      <h:outputLabel for="regisID" value="Registration ID:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="regisID"
                                    value="#{backing_OwnerDetails.regisID}"
                                    styleClass="field-output"/>
                    </div>
                    <div class="field-row">
                      <h:outputLabel for="location" value="Location:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="location"
                                    value="#{backing_OwnerDetails.location}"
                                    styleClass="field-output"/>
                    </div>
                  </div>
                  <div class="two-column-row">
                    <div class="field-row">
                      <h:outputLabel for="regisDate" value="Registration Date:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="regisDate"
                                    value="#{backing_OwnerDetails.regisDate}"
                                    styleClass="field-output"/>
                    </div>
                    <div class="field-row">
                      <h:outputLabel for="registrar" value="Registrar:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="registrar"
                                    value="#{backing_OwnerDetails.registrar}"
                                    styleClass="field-output"/>
                    </div>
                  </div>
                </h:panelGroup>
                <!-- 4. Vehicle Details Card -->
                <h:panelGroup layout="block" styleClass="card vehicle-card"
                              rendered="#{backing_OwnerDetails.showCard}">
                  <h:outputText value="Vehicle Details"
                                styleClass="card-header"/>
                  <div class="two-column-row">
                    <div class="field-row">
                      <h:outputLabel for="vehID" value="Vehicle ID:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="vehID"
                                    value="#{backing_OwnerDetails.vehID}"
                                    styleClass="field-output"/>
                    </div>
                    <div class="field-row">
                      <h:outputLabel for="brand" value="Brand:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="brand"
                                    value="#{backing_OwnerDetails.brand}"
                                    styleClass="field-output"/>
                    </div>
                  </div>
                  <div class="two-column-row">
                    <div class="field-row">
                      <h:outputLabel for="model" value="Model:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="model"
                                    value="#{backing_OwnerDetails.model}"
                                    styleClass="field-output"/>
                    </div>
                    <div class="field-row">
                      <h:outputLabel for="vehType" value="Vehicle Type:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="vehType"
                                    value="#{backing_OwnerDetails.vehType}"
                                    styleClass="field-output"/>
                    </div>
                  </div>
                  <div class="two-column-row">
                    <div class="field-row">
                      <h:outputLabel for="fuel" value="Fuel:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="fuel"
                                    value="#{backing_OwnerDetails.fuel}"
                                    styleClass="field-output"/>
                    </div>
                    <div class="field-row">
                      <h:outputLabel for="exhaust" value="Exhaust:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="exhaust"
                                    value="#{backing_OwnerDetails.exhaust}"
                                    styleClass="field-output"/>
                    </div>
                  </div>
                  <div class="two-column-row">
                    <div class="field-row">
                      <h:outputLabel for="color" value="Color:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="color"
                                    value="#{backing_OwnerDetails.color}"
                                    styleClass="field-output"/>
                    </div>
                    <div class="field-row">
                      <h:outputLabel for="manufDate" value="Manufacture Date:"
                                     styleClass="field-label"/>
                       
                      <h:outputText id="manufDate"
                                    value="#{backing_OwnerDetails.manufDate}"
                                    styleClass="field-output"/>
                    </div>
                  </div>
                </h:panelGroup>
              </h:form>
            </div>
          </div>
        </div>
      </body>
    </html>
  </f:view>
  <!--oracle-jdev-comment:auto-binding-backing-bean-name:backing_OwnerDetails-->
</jsp:root>
